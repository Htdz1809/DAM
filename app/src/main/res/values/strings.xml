    <!-- Wilaya Names and Descriptions -->
    <string name="wilaya_algiers">Algiers</string>
    <string name="wilaya_algiers_ar">الجزائر</string>
    <string name="wilaya_algiers_desc_en">The capital city of Algeria, known for its rich history, Mediterranean coastline, and vibrant culture.</string>
    <string name="wilaya_algiers_desc_ar">عاصمة الجزائر، تشتهر بتاريخها الغني وساحلها المتوسطي وثقافتها النابضة بالحياة.</string>

    <string name="wilaya_oran">Oran</string>
    <string name="wilaya_oran_ar">وهران</string>
    <string name="wilaya_oran_desc_en">A major port city with a rich cultural heritage and beautiful Mediterranean architecture.</string>
    <string name="wilaya_oran_desc_ar">مدينة مينائية كبرى تتميز بتراث ثقافي غني وعمارة متوسطية جميلة.</string>

    <string name="wilaya_constantine">Constantine</string>
    <string name="wilaya_constantine_ar">قسنطينة</string>
    <string name="wilaya_constantine_desc_en">Known as the City of Bridges, featuring dramatic cliffs and historic architecture.</string>
    <string name="wilaya_constantine_desc_ar">المعروفة باسم مدينة الجسور، تتميز بمنحدرات درامية وعمارة تاريخية.</string>

    <string name="wilaya_annaba">Annaba</string>
    <string name="wilaya_annaba_ar">عنابة</string>
    <string name="wilaya_annaba_desc_en">A coastal city with beautiful beaches and ancient Roman ruins.</string>
    <string name="wilaya_annaba_desc_ar">مدينة ساحلية تتميز بشواطئ جميلة وآثار رومانية قديمة.</string>

    <string name="wilaya_blida">Blida</string>
    <string name="wilaya_blida_ar">البليدة</string>
    <string name="wilaya_blida_desc_en">Known as the City of Roses, surrounded by beautiful gardens and mountains.</string>
    <string name="wilaya_blida_desc_ar">المعروفة باسم مدينة الورود، محاطة بحدائق وجبال جميلة.</string>

    <string name="wilaya_setif">Setif</string>
    <string name="wilaya_setif_ar">سطيف</string>
    <string name="wilaya_setif_desc_en">A historic city with rich agricultural heritage and Roman ruins.</string>
    <string name="wilaya_setif_desc_ar">مدينة تاريخية تتميز بتراث زراعي غني وآثار رومانية.</string>

    <string name="wilaya_batna">Batna</string>
    <string name="wilaya_batna_ar">باتنة</string>
    <string name="wilaya_batna_desc_en">Gateway to the Aures Mountains and home to ancient Roman ruins.</string>
    <string name="wilaya_batna_desc_ar">بوابة جبال الأوراس وموطن للآثار الرومانية القديمة.</string>

    <string name="wilaya_bejaia">Bejaia</string>
    <string name="wilaya_bejaia_ar">بجاية</string>
    <string name="wilaya_bejaia_desc_en">A coastal city with beautiful beaches and rich cultural heritage.</string>
    <string name="wilaya_bejaia_desc_ar">مدينة ساحلية تتميز بشواطئ جميلة وتراث ثقافي غني.</string>

    <string name="wilaya_skikda">Skikda</string>
    <string name="wilaya_skikda_ar">سكيكدة</string>
    <string name="wilaya_skikda_desc_en">A port city with beautiful Mediterranean coastline and historic sites.</string>
    <string name="wilaya_skikda_desc_ar">مدينة مينائية تتميز بساحل متوسطي جميل ومواقع تاريخية.</string>

    <string name="wilaya_tlemcen">Tlemcen</string>
    <string name="wilaya_tlemcen_ar">تلمسان</string>
    <string name="wilaya_tlemcen_desc_en">Known for its Islamic architecture and rich cultural heritage.</string>
    <string name="wilaya_tlemcen_desc_ar">تشتهر بعمارتها الإسلامية وتراثها الثقافي الغني.</string>

